	<queryString>
		<![CDATA[SELECT
				ciudad.nombre_ciudad,
				juicios.juicio_referido_titulo_credito,
				clientes.nombres_clientes,
				clientes.identificacion_clientes,
				documentos.fecha_emision_documentos,
				documentos.hora_emision_documentos,
				usuarios.nombre_usuarios, 
				asignacion_secretarios_usuarios_view.secretarios, 
				asignacion_secretarios_usuarios_view.impulsores
				FROM
				public.documentos,
				public.ciudad,
				public.juicios,
				public.clientes,
				public.asignacion_secretarios_usuarios_view, 
				public.usuarios
				WHERE
				ciudad.id_ciudad = documentos.id_ciudad AND
				juicios.id_juicios = documentos.id_juicio AND
				clientes.id_clientes = juicios.id_clientes AND
				usuarios.id_usuarios = asignacion_secretarios_usuarios_view.id_asignacion_secretarios
				AND documentos.identificador ='']]>
	</queryString>
